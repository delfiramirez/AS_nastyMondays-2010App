package com.segonquart{	import flash.display.MovieClip;	import flash.display.Loader;	import flash.events.Event;	import flash.net.URLRequest;		public class Added extends MovieClip	{		private var vloader:Loader;				public function Added()		{			bg_mc.stop();			stage.frameRate=31;			loadSWF();		}				private function loadSWF():void		{			var swf:URLRequest=new URLRequest();			swf.url='movies/morph7.swf';						vloader=new Loader();			vloader.contentLoaderInfo.addEventListener(Event.COMPLETE,done);			vloader.load(swf);			vloader.x=0;			vloader.y=0;		}				private function done(evt:Event):void		{			addChild(vloader);		}	}}